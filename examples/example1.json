{
    "tables": [
		{
			"table_name": "address",
            "file_name": "abc",
            "columns": [
                {
                    "name": "address_id",
                    "type": "str",
					"constraint": ["PK"],
                    "rule":  "uuid"
                },
                {
                    "name": "address",
                    "type": "varchar(50)",
                    "nullable": true,
                    "rule": {
                        "type": "address",
                        "multi_line": false
                    }
                },
                {
                    "name": "comment",
                    "type": "varchar(10)"
                },
                {
                    "name": "comment_1",
                    "type": "str",
                    "nullable": true,
                    "rule": "word",
                    "default": "this is default",
                    "null_percentage": 50
                }
			]
		},
        {
            "table_name": "customer",
            "columns": [
                {
                    "name": "customer_id",
                    "type": "int",
					"constraint": ["PK"],
                    "rule": {
                        "type": "range",
                        "min": 0
                    }
                },
                {
                    "name": "first_name",
                    "type": "str",
                    "nullable": true,
                    "rule": "first_name"
                },
                {
                    "name": "last_name",
                    "type": "str",
                    "nullable": true,
                    "rule": "last_name"
                },
                {
                    "name": "email",
                    "type": "str",
                    "nullable": true,
                    "rule": {
                        "type": "email",
                        "regex": "^[\\w.-]+@[\\w.-]+\\.\\w{2,4}$"
                    },
                    "sensitivity": "PII"
                },
                {
                    "name": "phone_number",
                    "type": "str",
                    "nullable": true,
                    "rule": {
                        "type": "regex",
                        "regex": "[0-9]{10,15}$"
                    }
                },
                {
                    "name": "date_of_birth",
                    "type": "date",
                    "nullable": true,
                    "rule": {
                        "type": "date_range",
                        "start": "1950-01-01",
                        "format": "YYYY-MM"
                    }
                },
                {
                    "name": "registration_date",
                    "type": "timestamp",
                    "nullable": true,
                    "rule": {
                        "type": "timestamp_range",
                        "start": "1950-01-01 00:00:00",
                        "format": "YYYY-MM-DDTHH:mm:ss"
                    },
                    "default": "2020-01-01"
                },
                {
                    "name": "address_id",
                    "type": "str",
                    "nullable": true
                },
                {
                    "name": "age",
                    "type": "int",
                    "nullable": true,
                    "rule": {
                        "type": "range",
                        "min": 18,
                        "max": 90
                    }
                },
                {
                    "name": "income",
                    "type": "decimal(10,2)"
                },
                {
                    "name": "gender",
                    "type": "str",
                    "nullable": true,
                    "rule": {
                        "type": "choice",
                        "value": [
                            "Male",
                            "Female",
                            "Other",
                            "Not Specified"
                        ],
                        "probabilities": {
                            "Male": 0.45,
                            "Female": 0.45,
                            "Other": 0.05,
                            "Not Specified": 0.05
                        }
                    },
                    "default": "Not Specified"
                }
            ],
            "foreign_keys": [
                {
                    "parent_table": "address",
                    "parent_column": "address_id",
                    "child_column": "address_id",
                    "relationship_type": "one_to_many",
                    "nullable": false
                }
            ]
        }
    ],
    "locale": "en_US",
    "rows": 20000,
    "output": {
        "format": "dsv",
        "enable_fixed_width": true,
        "directory": "./output/{timestamp}",
        "filename_template": "{table_name}",
        "delimiter": "|",
        "alignment": "left",
        "numeric_alignment": "left",
        "default_column_width": 30
      }
}

