{
    "tables": [
		{
			"table_name": "address",
            "file_name": "abc",
            "columns": [
                {
                    "name": "address_id",
                    "type": "str",
					"constraint": ["PK"],
                    "rule":  "uuid"
                },
                {
                    "name": "address",
                    "type": "varchar(50)",
                    "nullable": true,
                    "rule": {
                        "type": "address",
                        "multi_line": false
                    }
                },
                {
                    "name": "comment",
                    "type": "varchar(10)"
                },
                {
                    "name": "comment_1",
                    "type": "str",
                    "nullable": true,
                    "rule": "word",
                    "default": "this is default",
                    "null_percentage": 50
                }
			]
		},
        {
            "table_name": "customer",
            "columns": [
                {
                    "name": "customer_id",
                    "type": "str",
					"constraint": ["PK"],
                    "rule": {
                      "type": "regex",
                      "regex": "^\\d{16}"
                    }
                },
                {
                    "name": "first_name",
                    "type": "str",
                    "nullable": true,
                    "rule": "first_name"
                },
                {
                    "name": "last_name",
                    "type": "str",
                    "nullable": true,
                    "rule": "last_name"
                },
                {
                    "name": "email",
                    "type": "str",
                    "nullable": true,
                    "rule": {
                        "type": "email",
                        "regex": "^[\\w.-]+@[\\w.-]+\\.\\w{2,4}$"
                    },
                    "sensitivity": "PII"
                },
                {
                    "name": "phone_number",
                    "type": "str",
                    "nullable": true,
                    "rule": {
                        "type": "regex",
                        "regex": "[0-9]{10,15}$"
                    }
                },
                {
                    "name": "date_of_birth",
                    "type": "date",
                    "nullable": true,
                    "rule": {
                        "type": "date_range",
                        "start": "1950-01-01",
                        "format": "YYYY-MM"
                    }
                },
                {
                    "name": "registration_date",
                    "type": "timestamp",
                    "nullable": true,
                    "rule": {
                        "type": "timestamp_range",
                        "start": "1950-01-01 00:00:00",
                        "format": "YYYY-MM-DDTHH:mm:ss"
                    },
                    "default": "2020-01-01"
                },
                {
                    "name": "address_id",
                    "type": "str",
                    "nullable": true
                },
                {
                    "name": "age",
                    "type": "int",
                    "nullable": true,
                    "rule": {
                        "type": "range",
                        "min": 18,
                        "max": 90
                    }
                },
                {
                    "name": "income",
                    "type": "decimal(10,4)"
                },
                {
                    "name": "gender",
                    "type": "str",
                    "nullable": true,
                    "rule": {
                        "type": "choice",
                        "value": [
                            "Male",
                            "Female",
                            "Other",
                            "Not Specified"
                        ],
                        "probabilities": {
                            "Male": 0.45,
                            "Female": 0.45,
                            "Other": 0.05,
                            "Not Specified": 0.05
                        }
                    },
                    "default": "Not Specified"
                }
            ],
            "foreign_keys": [
                {
                    "parent_table": "address",
                    "parent_column": "address_id",
                    "child_column": "address_id",
                    "relationship_type": "one_to_one",
                    "nullable": false
                }
            ]
        },
        {
      "table_name": "order_header",
      "columns": [
        {
          "name": "order_id",
          "type": "str",
          "constraint": ["PK"],
          "nullable": false,
          "rule": {
            "prefix": "ORD-",
            "type": "range",
            "min": 1000000,
            "max": 9999999
          },
          "length": 11
        },
        {
          "name": "customer_id",
          "type": "int",
          "nullable": false
        },
        {
          "name": "order_date",
          "type": "date",
          "nullable": false,
          "rule": {
            "type": "date_range",
            "start": "2023-01-01",
            "end": "2024-12-31"
          }
        },
        {
          "name": "order_time",
          "type": "str",
          "nullable": false,
          "rule": {
            "type": "choice",
            "value": [
              "09:00:00", "10:30:00", "12:15:00", "14:45:00", "16:20:00",
              "18:30:00", "20:10:00", "22:00:00"
            ]
          }
        },
        {
          "name": "status",
          "type": "str",
          "nullable": false,
          "rule": {
            "type": "choice",
            "value": ["PENDING", "CONFIRMED", "PROCESSING", "SHIPPED", "DELIVERED", "CANCELLED", "RETURNED"],
            "probabilities": {
              "PENDING": 0.05,
              "CONFIRMED": 0.1,
              "PROCESSING": 0.15,
              "SHIPPED": 0.2,
              "DELIVERED": 0.45,
              "CANCELLED": 0.03,
              "RETURNED": 0.02
            }
          },
          "default": "PENDING"
        },
        {
          "name": "subtotal",
          "type": "float",
          "nullable": false,
          "rule": {
            "type": "range",
            "min": 10.00,
            "max": 10000.00
          }
        },
        {
          "name": "tax_amount",
          "type": "float",
          "nullable": false,
          "rule": {
            "type": "range",
            "min": 0.50,
            "max": 800.00
          }
        },
        {
          "name": "shipping_cost",
          "type": "float",
          "nullable": false,
          "rule": {
            "type": "range",
            "min": 0.00,
            "max": 50.00
          }
        },
        {
          "name": "total_amount",
          "type": "float",
          "nullable": false,
          "rule": {
            "type": "range",
            "min": 10.50,
            "max": 10850.00
          }
        },
        {
          "name": "payment_method",
          "type": "str",
          "nullable": false,
          "rule": {
            "type": "choice",
            "value": ["CREDIT_CARD", "DEBIT_CARD", "PAYPAL", "APPLE_PAY", "GOOGLE_PAY", "BANK_TRANSFER", "CASH_ON_DELIVERY"],
            "probabilities": {
              "CREDIT_CARD": 0.4,
              "DEBIT_CARD": 0.25,
              "PAYPAL": 0.15,
              "APPLE_PAY": 0.08,
              "GOOGLE_PAY": 0.07,
              "BANK_TRANSFER": 0.03,
              "CASH_ON_DELIVERY": 0.02
            }
          }
        },
        {
          "name": "shipping_address_id",
          "type": "str",
          "nullable": true
        },
        {
          "name": "billing_address_id",
          "type": "str",
          "nullable": true
        },
        {
          "name": "notes",
          "type": "str",
          "nullable": true,
          "rule": {
            "type": "choice",
            "value": [
              "Rush delivery requested", "Gift wrapping required", "Fragile items",
              "Leave at front door", "Contact before delivery", "Business delivery",
              "Weekend delivery preferred", "Special handling required"
            ]
          },
          "length": {"min": 10, "max": 150}
        }
      ],
      "foreign_keys": [
        {
          "parent_table": "customer",
          "parent_column": "customer_id",
          "child_column": "customer_id",
          "relationship_type": "many_to_one",
          "nullable": false
        },
        {
          "parent_table": "address",
          "parent_column": "address_id",
          "child_column": "shipping_address_id",
          "relationship_type": "one_to_one",
          "nullable": true
        },
        {
          "parent_table": "address",
          "parent_column": "address_id",
          "child_column": "billing_address_id",
          "relationship_type": "one_to_one",
          "nullable": true
        }
      ]
    }
    ],
    "locale": "en_US",
    "rows": 20000,
    "output": {
        "format": "csv",
        "enable_fixed_width": false,
        "directory": "./output/{timestamp}",
        "filename_template": "{table_name}",
        "alignment": "left",
        "numeric_alignment": "left",
        "default_column_width": 30
      }
}

