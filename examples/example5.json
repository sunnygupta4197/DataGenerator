{
  "environment": "development",
  "locale": "en_GB",
  "rows": 1,
  "tables": [
    {
      "table_name": "address",
      "columns": [
        {
          "name": "address_id",
          "type": "str",
          "constraints": ["PK"],
          "rule": "uuid"
        },
        {
          "name": "address",
          "type": "str",
          "nullable": true,
          "rule": "address"
        },
        {
          "name": "country_detail",
          "type": "str",
          "nullable": false,
          "rule": {
            "type": "ai_generated",
            "description": "Generate longitude and latitude coordinates for locations",
            "context_columns": ["country", "continent"],
            "max_length": 100
          }
        }
      ]
    }
  ],
  "ai": {
    "primary_provider": "mistral",
    "enable_fallback": false,
    "shared_cache_size": 200,
    "openai": {
      "enabled": false,
      "api_key": "",
      "model": "gpt-3.5-turbo",
      "max_tokens": 150,
      "temperature": 0.7,
      "cache_size": 200,
      "timeout_seconds": 60,
      "retry_attempts": 3,
      "fallback_enabled": true,
      "cost_limit_usd": 100.0
    },
    "mistral": {
      "enabled": true,
      "api_key": "",
      "model": "mistral-small",
      "max_tokens": 150,
      "temperature": 0.7,
      "cache_size": 200,
      "timeout_seconds": 60,
      "retry_attempts": 3,
      "fallback_enabled": true,
      "cost_limit_usd": 50.0
    }
  },
  "performance": {
    "max_workers": 2,
    "batch_size": 1000,
    "max_memory_mb": 500,
    "enable_parallel": true,
    "enable_streaming": false
  },
  "security": {
    "enable_data_masking": false,
    "enable_encryption": false,
    "audit_enabled": false
  },
  "output": {
    "format": "csv",
    "directory": "./output",
    "filename_template": "{table_name}_{timestamp}",
    "include_header": true,
    "append_timestamp": true
  },
  "validation": {
    "strict_mode": false,
    "max_validation_errors": 50,
    "enable_business_rules": true,
    "enable_data_quality_analysis": false
  },
  "logging": {
    "level": "INFO",
    "console_output": true,
    "log_to_file": false
  }
}
